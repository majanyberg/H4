function find(bst, name) {
    // your answer here
    return is_empty_tree(bst) 
           ? false
           : name === entry(bst) 
           ? true
           : name < entry(bst)
           ? find(left_branch(bst), name)
           : find(right_branch(bst), name);
}


// Test

find(pkd_names, "Nyberg, Maja");
